                     -L$ngx_addon_dir/build/extend/lib/ -Wl,-Bstatic                   \
                     -lxml2 -liconv -lbz2 -lzookeeper_mt  -llzma                       \
                     -lpcre -lpcrecpp -lpcreposix  -lssl  -lcrypto                     \
